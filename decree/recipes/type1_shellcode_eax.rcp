# Set GP and EIP with shellcode
# mov eax $gp
# mov ebx, $eip
# jmp ebx
*regMask = 0xffffffff*
*ipMask = 0xffffffff*
*type 1*
*gp = EAX*
*EIP points to executable memory*
[EIP+0] == 0xb8
[EIP+1] == $gp[0]
[EIP+2] == $gp[1]
[EIP+3] == $gp[2]
[EIP+4] == $gp[3]
[EIP+5] == 0xbb
[EIP+6] == $eip[0]
[EIP+7] == $eip[1]
[EIP+8] == $eip[2]
[EIP+9] == $eip[3]
[EIP+10] == 0xff
[EIP+11] == 0xe3
